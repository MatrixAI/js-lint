{
  "name": "@matrixai/lint",
  "version": "0.0.1",
  "author": "Roger Qiu",
  "description": "Org wide custom eslint rules",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/MatrixAI/js-eslint.git"
  },
  "type": "module",
  "exports": {
    "./config": {
      "import": "./dist/configs/matrixai-config-bundle.js"
    }
  },
  "imports": {
    "#*": "./dist/*"
  },
  "./package.json": "./package.json",
  "bin": {
  "matrixai-lint": "./dist/bin/lint.js"
  },
  "scripts": {
    "prepare": "tsc -p ./tsconfig.build.json",
    "build": "shx rm -rf ./dist && tsc -p ./tsconfig.build.json && shx chmod +x dist/bin/lint.js",
    "postversion": "npm install --package-lock-only --ignore-scripts --silent",
    "tsx": "tsx",
    "lint": "eslint '{src,tests,scripts,benches}/**/*.{js,mjs,ts,mts,jsx,tsx}'",
    "lintfix": "eslint '{src,tests,scripts,benches}/**/*.{js,mjs,ts,mts,jsx,tsx}' --fix",
    "lint-shell": "find ./src ./tests ./scripts -type f -regextype posix-extended -regex '.*\\.(sh)' -exec shellcheck {} +",
    "docs": "shx rm -rf ./docs && typedoc --gitRevision master --tsconfig ./tsconfig.build.json --out ./docs src",
    "test": "node ./scripts/test.mjs"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.16.0",
    "@swc/core": "1.3.82",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@typescript-eslint/utils": "^8.26.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "@eslint/eslintrc": "^3.2.0",
    "jest": "^29.6.2",
    "jest-extended": "^4.0.2",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.0",
    "shx": "^0.3.4",
    "tsconfig-paths": "^3.9.0",
    "tsx": "^3.12.7",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  }
}
